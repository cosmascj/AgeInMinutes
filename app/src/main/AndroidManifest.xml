<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.getminutesage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/custom_launcher_four"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/custom_launcher_four_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GetMinutesAge">
        <activity
            android:name=".IntroActivity"
            android:windowSoftInputMode="stateHidden"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.App.Starting.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">

        </activity>
    </application>

</manifest>